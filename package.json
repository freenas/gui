{
  "name": "freenas-10-gui",
  "version": "0.3.0",
  "dependencies": {
    "blue-shark": "freenas/blue-shark#95c48ab1fbb282e81b8c5320945631d6032af666",
    "collections": "montagejs/collections#90210529c7b01ed373b65d4a60409f5edd36a93a",
    "d3": "^3.5.17",
    "digit": "montagejs/digit",
    "frb": "montagejs/frb#66ba657ae8e7ada7658df3beb6e924c500b7259c",
    "montage": "montagejs/montage#d8936b90fafb3a1d9bc01ff8924208513418b408",
    "montage-data": "montagestudio/montage-data#647f5208fcefbc89c405d1afea205c8184fddeda",
    "plottable": "~2.2.0",
    "validators": "~0.3.1",
    "xterm": "^1.0.0",
    "maphilight": "~1.2.3"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "commander": "~2.9.0",
    "cssnano": "^3.7.1",
    "gulp": "^3.9.1",
    "gulp-header": "^1.8.5",
    "gulp-newer": "^1.2.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-typescript": "^3.1.3",
    "js-beautify": "^1.6.3",
    "json.sortify": "^2.0.2",
    "montage-testing": "~0.4.0",
    "mop": "montagejs/mop#dc468f661b68b7bf6fb3b01fcee64b0c4adbcf1b",
    "mr": "montagejs/mr#53633e56f3e9d00c5a311d993098ddd89ed9be80",
    "postcss-cssnext": "^2.7.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^8.1.2",
    "progress": "~1.1.8",
    "prompt": "~0.3.0",
    "rimraf": "~2.5.2",
    "typescript": "^2.0.10",
    "ws": "~1.1.1"
  },
  "exclude": [
    "run-tests.html",
    "test",
    "tools",
    "nas_ports"
  ],
  "bundle": [
    "ui/main.reel"
  ],
  "shard": 3,
  "scripts": {
    "update-montage-data-cache": "node ./tools/cli/update-montage-data-cache.js",
    "clean-montage-data-cache": "node ./tools/cli/clean-montage-data-cache.js",
    "generate-descriptors": "node ./tools/cli/generate.js descriptors -t ./core/model/descriptors",
    "generate-model": "node ./tools/cli/generate.js model ./core/model/descriptors/ -p ./core/model/descriptors -t ./core/model",
    "generate-enumerations": "node ./tools/cli/generate.js enumerations -t ./core/model/enumerations",
    "generate-services": "node ./tools/cli/generate.js services -t ./core/model",
    "generate-events": "node ./tools/cli/generate-events.js -t ./core/model",
    "build": "node ./tools/build.js",
    "warnings": "node ./tools/cli/generate.js descriptors -w --no-save",
    "create-inspector": "minit create:component --extends-name AbstractInspector --extends-module-id ui/abstract/abstract-inspector -n",
    "create-dao": "minit create:module --extends-name AbstractDao --extends-module-id core/dao/abstract-dao -d core/dao -n",
    "create-repository": "minit create:module --extends-name AbstractRepository --extends-module-id core/repository/abstract-repository -d core/repository -n",
    "create-section-service": "minit create:module --extends-name AbstractSectionService --extends-module-id core/service/section/abstract-section-service -d core/service/section -n"
  }
}
